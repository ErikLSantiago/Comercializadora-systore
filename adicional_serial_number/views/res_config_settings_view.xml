<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="res_config_settings_view_form_adicional_sn" model="ir.ui.view">
      <field name="name">res.config.settings.view.form.adicional.sn</field>
      <field name="model">res.config.settings</field>
      <field name="inherit_id" ref="base.view_res_config_settings"/>
      <field name="arch" type="xml">
        <xpath expr="//div[@id='settings']" position="inside">
          <div class="app_settings_block" data-string="Adicional Serial Number" data-key="adicional_sn">
            <h2>Adicional Serial Number</h2>
            <div class="row mt16 o_settings_container">
              <div class="col-12 col-lg-6 o_setting_box">
                <div class="o_setting_left_pane"/>
                <div class="o_setting_right_pane">
                  <label for="adicional_sn_version"/>
                  <div class="text-muted">Versión instalada del módulo</div>
                  <div class="content-group">
                    <field name="adicional_sn_version" readonly="1"/>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </xpath>
      </field>
    </record>
  </data>
</odoo>
