<odoo>
  <record id="view_product_template_form_inherit_wqr" model="ir.ui.view">
    <field name="name">product.template.form.wqr.quote_publish</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_form_view"/>
    <field name="arch" type="xml">
      <xpath expr="//group[@name='ecommerce']" position="inside">
        <field name="quote_publish"/>
      </xpath>
      <xpath expr="//sheet//group[1]" position="inside">
        <field name="quote_publish" attrs="{'invisible': [('quote_publish','=',True)]}"/>
      </xpath>
    </field>
  </record>
</odoo>
