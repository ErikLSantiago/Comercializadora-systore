<odoo>
  <record id="seq_meat_cutting_order" model="ir.sequence">
    <field name="name">Orden Despiece</field>
    <field name="code">meat.cutting.order</field>
    <field name="prefix">DSP/%(year)s/</field>
    <field name="padding">5</field>
    <field name="company_id" eval="False"/>
  </record>

<record id="seq_meat_cutting_serial" model="ir.sequence">
  <field name="name">Meat Cutting Serial</field>
  <field name="code">meat_cutting.serial</field>
  <field name="prefix">C-</field>
  <field name="padding">4</field>
  <field name="company_id" eval="False"/>
</record>

  <!-- Ubicación técnica para despiece -->
  <record id="stock_location_meat_cutting_production" model="stock.location">
    <field name="name">Despiece / Producción</field>
    <field name="usage">production</field>
    <field name="active">True</field>
  </record>

  <record id="picking_type_meat_cutting" model="stock.picking.type">
    <field name="name">Despiece</field>
    <field name="code">internal</field>
    <field name="sequence_code">MEAT.CUTTING</field>
    <field name="use_create_lots">True</field>
    <field name="use_existing_lots">True</field>
  </record>
</odoo>
