<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record id="view_move_line_list_adicional_sn" model="ir.ui.view">
      <field name="name">adicional.sn.product.line.list</field>
      <field name="model">adicional.sn.product.line</field>
      <field name="arch" type="xml">
        <list string="Productos (seriales adicionales)" create="false">
          <field name="picking_id" optional="show"/>
          <field name="product_id"/>
          <field name="demand_total"/>
          <field name="reserved_total"/>
          <field name="captured_label"/>
          <button name="action_open_serial_product_wizard" type="object" icon="fa-cubes" string="Seriales (producto)"/>
        </list>
      </field>
    </record>

    <record id="action_move_lines_by_picking_adicional_sn" model="ir.actions.act_window">
      <field name="name">Productos de la operaci√≥n</field>
      <field name="res_model">adicional.sn.product.line</field>
      <field name="view_mode">list,form</field>
      <field name="view_id" ref="view_move_line_list_adicional_sn"/>
      
      <field name="context">{}</field>
    </record>
  </data>
</odoo>
